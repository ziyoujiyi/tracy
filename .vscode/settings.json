        "${workspaceFolder}/capture",
        "${workspaceFolder}/csvexport",
        "${workspaceFolder}/import",
        "${workspaceFolder}/update",
        "${workspaceFolder}/test",
        "${workspaceFolder}",
    ],
    "cmake.buildDirectory": "${sourceDirectory}/build",
